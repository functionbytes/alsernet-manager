{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "aerni/cloudflared": "^1.1",
        "artesaos/seotools": "^1.3",
        "ashallendesign/email-utilities": "^1.0",
        "bacon/bacon-qr-code": "^2.0",
        "barryvdh/laravel-dompdf": "^3.1",
        "barryvdh/laravel-ide-helper": "^3.6",
        "barryvdh/laravel-translation-manager": "^0.6.8",
        "botman/botman": "^2.8",
        "botman/driver-web": "^1.5",
        "deeplcom/deepl-php": "^1.11",
        "doctrine/dbal": "^4.3",
        "egulias/email-validator": "^4.0",
        "ezyang/htmlpurifier": "^4.17",
        "geoip2/geoip2": "^3.3",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/psr7": "2.7",
        "ifsnop/mysqldump-php": "^2.12",
        "intervention/image": "*",
        "kriswallsmith/buzz": "^1.3",
        "kub-at/php-simple-html-dom-parser": "^1.9",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.40",
        "laravel/mcp": "^0.4.0",
        "laravel/pulse": "^1.4",
        "laravel/reverb": "^1.4",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.15",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.6",
        "league/csv": "^9.21",
        "league/pipeline": "^1.0",
        "maatwebsite/excel": "^3.1",
        "milon/barcode": "^11.0|^12.0",
        "nesbot/carbon": "^3.8",
        "phpmailer/phpmailer": "^6.9",
        "picqer/php-barcode-generator": "^3.2",
        "propaganistas/laravel-disposable-email": "^2.2",
        "pusher/pusher-php-server": "*",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.6",
        "setasign/fpdi-tcpdf": "^2.3",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "soundasleep/html2text": "~1.1",
        "spatie/laravel-activitylog": "^4.9|^5.0",
        "spatie/laravel-backup": "^9.3",
        "spatie/laravel-cookie-consent": "^3.2|^4.0",
        "spatie/laravel-health": "^1.34",
        "spatie/laravel-medialibrary": "^10.0|^11.0",
        "spatie/laravel-permission": "^6.0|^7.0",
        "spatie/laravel-query-builder": "^6.3",
        "spatie/laravel-rate-limited-job-middleware": "^2.8",
        "spatie/ssl-certificate": "^2.6",
        "symfony/finder": "^7.2",
        "symfony/mime": "^7.2",
        "torann/geoip": "^3.0",
        "twig/twig": "^3.19",
        "tymon/jwt-auth": "^2.2",
        "webklex/laravel-imap": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/boost": "^1.8",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/Helper.php",
            "app/Helpers/Namespaced.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/Helpers/Helper.php",
            "app/Helpers/Namespaced.php"
        ]
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "npm install",
            "npm run build"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "docs": [
            "@php artisan docs:generate-commands"
        ],
        "setup:hooks": [
            "bash manual/setup-hooks.sh"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
